                      "items": { "type": "object", "additionalProperties": true }
                    },
                    "nextToken": { "type": ["string","null"] }
                  },
                  "required": ["records"]
                }
              }
            }
          }
        }
      }
    },
    "/list/cycle": {
      "get": {
